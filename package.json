{
  "name": "sharedstreets",
  "version": "0.1.2",
  "description": "SharedStreets, a 'digital commons' for the street",
  "main": "index",
  "types": "index.d.ts",
  "browser": "dist/sharedstreets.min.js",
  "files": [
    "index.js",
    "index.ts",
    "index.d.ts",
    "dist"
  ],
  "scripts": {
    "pretest": "tsc",
    "test": "node test.js",
    "posttest": "tslint index.ts --project tsconfig.json",
    "docs": "documentation readme index.js --section=API",
    "benchmark": "benchmark bench.js",
    "build": "browserify index.js -s sharedstreets -o dist/sharedstreets.js",
    "postbuild": "uglifyjs dist/sharedstreets.js -o dist/sharedstreets.min.js -cm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharedstreets/sharedstreets.js.git"
  },
  "keywords": [
    "sharedstreets",
    "streets",
    "highway",
    "reference"
  ],
  "author": "SharedStreets",
  "contributors": [
    "Denis Carriere <@DenisCarriere>",
    "Kevin Webb <@kpwebb>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharedstreets/sharedstreets.js/issues"
  },
  "homepage": "https://github.com/sharedstreets/sharedstreets.js#readme",
  "devDependencies": {
    "@types/node": "*",
    "tape": "*",
    "tslint": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@turf/helpers": "6.0.0-beta.3",
    "@turf/invariant": "6.0.0-beta.3",
    "sharedstreets-types": "1.x"
  }
}
