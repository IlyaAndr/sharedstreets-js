    "test": "tsc && nyc ava --verbose",
    "docs": "tsc && documentation readme index.js --section=API"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharedstreets/sharedstreets.js.git"
  },
  "keywords": [
    "sharedstreets",
    "streets",
    "highway",
    "reference"
  ],
  "author": "SharedStreets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharedstreets/sharedstreets.js/issues"
  },
  "homepage": "https://github.com/sharedstreets/sharedstreets.js#readme",
  "devDependencies": {
    "@types/node": "*",
    "ava": "*",
    "documentation": "*",
    "nyc": "*",
    "rollup": "*",
    "rollup-plugin-node-resolve": "*",
    "rollup-plugin-typescript2": "*",
    "rollup-plugin-uglify": "*",
    "standard": "*",
    "tslint": "*",
    "typescript": "*"
  },
  "@std/esm": "js",
  "dependencies": {
    "@turf/helpers": "5.1.x",
    "@turf/invariant": "5.2.x",
    "@turf/meta": "5.2.x"
  }
