{
  "name": "sharedstreets",
  "version": "0.1.0",
  "description": "SharedStreets, a 'digital commons' for the street",
  "main": "dist/sharedstreets.js",
  "module": "dist/sharedstreets.mjs",
  "browser": "dist/sharedstreets.min.js",
  "files": [
    "index.ts",
    "index.js",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
