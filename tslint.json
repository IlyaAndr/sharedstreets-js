{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "semicolon": [true, "never"],
        "object-literal-sort-keys": [false],
        "max-line-length": [true, 150],
        "ordered-imports": [false],
        "arrow-parens": [false],
        "no-console": [false],
        "interface-name": [false]
    },
    "rulesDirectory": []
}